{
    "name": "spiritsaint/code-difference",
    "description": "A Laravel Nova field to view differences between two inputs.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "SpiritSaint\\CodeDifference\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SpiritSaint\\CodeDifference\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
